.noData {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1000;
}
.noData .img {
  width: 36.9vh;
  height: 36.9vh;
}
.noData .text-view {
  font-size: 5.5vh;
  color: #E6E6E6;
  margin-top: 4vh;
}
.noData .text-view text:nth-child(2) {
  color: #f4c901;
}
