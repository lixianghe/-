<view class="{{className}}albumCard" >
  <image class="image" src="{{datasource.src}}" ></image>
  <image wx:if="likePic" src="{{likePic[0]}}"></image>
  <view class="albumTit">
    <text class="vip" wx:if="{{datasource.isVip}}" catchtap="likeOne">VIP</text>
    <text>{{ datasource.title }}</text>
  </view>
</view>