.h .index {
  overflow: auto;
  padding-left: 7.3vh;
  box-sizing: border-box;
  height: 82vh;
  padding: 5.5vh 0;
  white-space: nowrap;
}

.h .latelyListen>image {
  width: 19.8vh;
  height: 19.8vh;
  margin-top: 17.8vh;
}
.h .latelyLtnTit {
  margin-top: 0.89vh;
  font-size: 4.58vh;
  color: #fff;
  line-height: 5.3vh;
}
.h .quickEntry{
  /* flex: 0 0 41.3vh; */
  width: 43.65vh;
  padding: 0 6.12vh 0 7.35vh;
  vertical-align: top;
  display: inline-block;
  height: 68.6vh;
  overflow: hidden;
  margin-right: 6.12vh;
  border-right: 0.58vh solid #1a1a1a;
} 
.h .quickEntrybox {
  text-align: center;
  background-color: #272727;
  background-size: 100%;
  border-radius: 2vh;
  margin-bottom: 6.3vh;
} 
.h .quickEntryicon {
  width: 14.76vh;
  height: 14.76vh;
  margin-top: 11%;
  vertical-align: bottom;
}
.h .mianContent {
  display: inline-block;
  vertical-align: top;
}
.h .albumCard {
  width: 46.6vh;
  display: inline-block;
  margin-right: 7.6vh;
  height: 68.6vh;
  overflow: hidden;
}
.h .imgB {
  width: 46.6vh;
  height: 46.6vh;
  border-radius: 2vh;
}
.albumTitB {
  margin-top: 6vh;
  height: 14vh;
}
.albumTitB .tb {
  height: 5vh;
  margin: 4vh 0;
}
.label-view {
  display: flex;
  color: #fff;
  font-size: 5.5vh;
  margin-bottom: 4.89vh;
  position: fixed;
}
.label-view .title {
  padding: 2vh 1vh;
  margin-right: 5vh;
  padding: 1.81vh 5.35vh 1.5vh 5.35vh;
  background-color: #282828;
  color: "#bebebe";
  border-radius: 5.5vh;
}
