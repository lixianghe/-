page {
  margin: 0;
  padding: 0;
}
.latelyListen {
  display: flex;
  overflow: auto;
  padding: 2vh;
  height: 83vh;
  align-items: center;
}
.latelyCard {
  flex: 0 0 58vh;
  margin-right: 3vh;
  height: 72vh;
  border-radius: 1.5vh;
  overflow: hidden;
}
.latelyCard>image {
  width: 58vh;
  height: 58vh;
  vertical-align: bottom;
}
.latelyTit {
  height: 14vh;
  background-color: rgba(0, 0, 0, 0.2);
  font-size: 4vh;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 1vh;
}