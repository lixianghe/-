page {
  margin: 0;
  padding: 0;
}
.personalCenter {
  display: flex;
  overflow: auto;
  padding: 2vh;
  height: 83vh;
  align-items: center;
}
.adminCenter {
  flex: 0 0 160rpx;
  margin-right: 30rpx;
  height: 72vh;
  overflow: hidden;
  text-align: center;
}
.adminCenter>image {
  width: 10vh;
  height: 10vh;
  margin-top: 20vh;
}
.adminCenterName {
  margin-top: 2vh;
  color: #fff;
  font-size: 3.3vh;
}
.adminBtnOut {
  margin-top: 3vh;
  font-size: 15rpx;
  background-color: #000;
  width: 75rpx;
  color: #fff;
  margin-left: 50%;
  transform: translateX(-50%);
  border-radius: 10rpx;
  padding: 3rpx 4rpx;
}
.adminBtnIn {
  margin-top: 3vh;
  font-size: 15rpx;
  width: 75rpx;
  color: #fff;
  border: 1px solid #fff;
  margin-left: 50%;
  transform: translateX(-50%);
  border-radius: 10rpx;
  padding: 3rpx 4rpx;
}
.cardListen{
  flex: 0 0 150rpx;
  margin-right: 30rpx;
  height: 72vh;
  border-radius: 1.5vh;
  overflow: hidden;
  background-size: 100%;
  text-align: center;
}
.cardListen>image {
  width: 10vh;
  height: 10vh;
  margin-top: 23vh;
}
.cardTit {
  margin-top: 2vh;
  font-size: 3vh;
  font-weight: bold;
}