.h .like {
  overflow: auto;
  padding-left: 18.7rpx;
  box-sizing: border-box;
  height: 210.1rpx;
  padding: 14.1rpx 23.1rpx;
  white-space: nowrap;
}
.h .mianContent {
  display: inline-block;
  vertical-align: top;
}
.h .mianContent .search {
  width: 320rpx;
  height: 28.2rpx;
  background: #272727;
  display: flex;
  align-items: center;
  border-radius: 28.2rpx;
  overflow: hidden;
  position: fixed;
  left: 23rpx;
  top: 11rpx;
}
.h .mianContent .search image {
  width: 25.63rpx;
  height: 25.63rpx;
  margin: 0 5,12rpx;
}
.h .mianContent .search input {
  background: #272727;
  height: 28.2rpx;
  width: 302.5rpx;
  color: #bbbbbb;
  font-size: 12.56rpx;
}
.h .cancel {
  width: 51.26rpx;
  height: 25.124rpx;
  line-height: 25.124rpx;
  border-radius: 25.124rpx;
  background: #ffac2d;
  color:#060401;
  font-size: 11.8rpx;
  position: fixed;
  left: 357rpx;
  top: 12.6rpx;
}
.h .albumCard {
  width: 92.26rpx;
  display: inline-block;
  margin-right: 19.5rpx;
  margin-top: 71.76rpx;
  height: 175.8rpx;
  overflow: hidden;
}
.h .imgB {
  width: 92.26rpx;
  height: 92.26rpx;
  border-radius: 5.126rpx;
}
.label-view {
  display: flex;
  color: #fff;
  font-size: 12.09rpx;
  margin-bottom: 12.54rpx;
  position: fixed;
  left: 23.06rpx;
  top: 47rpx;
}
.label-view .title {
  padding: 4.9rpx 13.7rpx;
  margin-right: 12.815rpx;
  background-color: #282828;
  border-radius: 14.09rpx;
}
.label-view .active-tap {
  color: #fa6500;
}

/* 卡片的样式 */
.halbumCard {
  width: 78rpx;
  display: inline-block;
  margin-right: 17.7rpx;
  margin-top: 66rpx;
  height: 159.8rpx;
  overflow: hidden;
  position: relative;
}
.halbumCard .image {
  width: 78rpx;
  height: 78rpx;
  border-radius: 4.66rpx;
  vertical-align: top;
}
.halbumCard .albumTit {
  margin-top: 8.155rpx;
  color: #ffffff;
  font-size: 11.8rpx;
  box-sizing: content-box;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}